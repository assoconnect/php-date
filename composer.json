{
    "name": "assoconnect/php-date",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/assoconnect/php-date/"
        }
    ],
    "type": "library",
    "description": "PHP object to handle dates without time",
    "keywords": ["php", "date"],
    "homepage": "https://github.com/assoconnect/php-date/",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AssoConnect\\PHPDate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AssoConnect\\PHPDate\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
		"squizlabs/php_codesniffer": "^3",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.1"
    },
    "require": {
        "php": "^7.4|^8.0"
    }
}
